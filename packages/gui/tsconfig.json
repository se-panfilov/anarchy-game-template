{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@Anarchy/Engine": ["packages/anarchy-engine/src/index.ts"],
      "@Anarchy/Engine/*": ["packages/anarchy-engine/src/*"],
      "@Anarchy/Shared": ["packages/anarchy-shared/src/index.ts"],
      "@Anarchy/Shared/*": ["packages/anarchy-shared/src/*"],
      "@Anarchy/i18n": ["packages/anarchy-i18n/src/index.ts"],
      "@Anarchy/i18n/*": ["packages/anarchy-i18n/src/*"],
      "@Anarchy/Tracking": ["packages/anarchy-tracking/src/index.ts"],
      "@Anarchy/Tracking/*": ["packages/anarchy-tracking/src/*"],
      "@Public/*": ["packages/gui/public/*"],
      "@Showcases/Menu": ["packages/menu/src/index.ts"],
      "@Showcases/Menu/*": ["packages/menu/src/*"],
      "@Showcases/GUI": ["packages/gui/src/index.ts"],
      "@Showcases/GUI/*": ["packages/gui/src/*"],
      "@Showcases/Shared": ["packages/shared/src/index.ts"],
      "@Showcases/Shared/*": ["packages/shared/src/*"],
      "@Showcases/i18n": ["packages/i18n/src/index.ts"],
      "@Showcases/i18n/*": ["packages/i18n/src/*"]
    },
    "typeRoots": ["./node_modules/@types", "./node_modules", "../../node_modules/@types", "../../node_modules", "./src/Engine/Types", "./types"]
  },
  "include": ["src", "vite.config.ts", "types"]
}
